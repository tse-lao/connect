{
    "manifest_version": 3,
    "name": "ConnectFast Extension",
    "version": "1.0",
    "description": "This extension does X, Y, and Z.",
    "host_permissions": [
        "https://*/*", 
        "http://*/*"
    ],
    "permissions": [
        "activeTab", 
        "cookies"
    ],
    "action":{
        "default_icon": "logo.png", 
        "default_popup": "popup.html", 
        "default_title": "CookieMonster"
    },
    "homepage_url": "https://www.example.com/my-extension",
    "icons": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }